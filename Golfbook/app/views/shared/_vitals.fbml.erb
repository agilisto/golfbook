<div class="section clearfix">
  <h3 class="box_header">My Vitals</h3>
  <div class="bar_chart simple">
    <h2 class="header">
      <div class="clearfix">
        <span class="title"><%= @user.rounds.count %></span>
        <span class="title_label"><%= link_to 'Rounds Played', user_view_rounds_url(:id => @user.id, :only_path => true) %></span>
      </div>
      <div class="clearfix">
        <span class="sub_title"/>
        <span class="title" style="clear:left;"><%= @user.courses.count %></span>
        <span class="title_label"><%= link_to 'Courses Played', :controller => :courses, :action => :courses_played %></span>
        <span class="sub_title"/>
      </div>
      <div style="clear: left; height: 0px;"/>
    </h2>
  </div>
</div>
