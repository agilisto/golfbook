<div class="module clearfix page_panel_header no_border">
  <div class="header_page_summary clearfix">
    <div class="thumb">
    </div>
    <div class="info">
      <h2>Handicaps</h2>
      <div>Welcome to Golfbook</div>
    </div>
  </div>
</div>

<%= render :partial => 'shared/home_menu.fbml.erb' %>

<div class="editor_panel clearfix">
  <div class="main_column clearfix">
    <h2 class="first_card_section"><fb:name uid="<%=@user.facebook_uid%>" capitalize="true" possessive="true" linked="false"/> Handicap</h2>
    <fb:iframe name='myhandicapframe' src="<%= url_for :controller => 'ezgraphics', :action => 'my_handicap', :id => @user.id, :friends => @uids, :only_path => false %>" frameborder=0 scrolling="no" height=400 width=500 >
    </fb:iframe>
    <% unless @uids.blank? %>
      <h2 class="first_card_section">Friend's Handicaps</h2>
      <fb:iframe name='friendhandicapframe' src="<%= url_for :controller => 'ezgraphics', :action => 'friends_handicap', :id => @uids, :only_path => false %>" height=400 width=500 frameborder=0 scrolling="no">
      </fb:iframe>
    <% end %>
  </div>
  <div class="side_column">
    <% vitals_box('My Rounds') do %>
      <ul class="small_list">
        <%= render :partial => 'shared/recent_round', :collection => @my_rounds, :locals => {:firstnameonly => true} %>
      </ul>
    <% end %>
    <% unless @friends_rounds.blank? %>
      <% vitals_box('Friends Rounds') do %>
        <ul class="small_list">
          <%= render :partial => 'shared/recent_round', :collection => @friends_rounds, :locals => {:firstnameonly => true} %>
        </ul>
      <% end %>
    <% end %>
  </div>
</div>