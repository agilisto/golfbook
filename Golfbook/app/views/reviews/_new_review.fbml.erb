<div id="course_reviews_add_new">
  <h3>The Wall</h3>
  <h5></h5>
  <% editor_form_for :review, @review, 
    :url => {:controller => :reviews, :action => :create, :id => @course.id }, :labelwidth=>"20"  do  |f| %>
      
    <%= f.text_area :review, :rows => 10 %>
         
    <fb:editor-buttonset>
      <%= f.submit_tag('Post This') %>
    </fb:editor-buttonset>
  
<% end %>
</div>
