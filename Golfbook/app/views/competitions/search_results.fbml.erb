<div class="module clearfix page_panel_header no_border">
  <div class="header_page_summary clearfix">
    <div class="thumb">
      <a href="<%= url_for(:action => :index)%>">
  <%= image_tag 'blue_flag_hole.png' %>
  </a>
      </div>
      <div class="info">
        <h2>Add Courses to <b><%= @competition.name %></b></h2>
        <div id="sub_menu">
          <%= link_to 'Back to '+@competition.name, {:action => :show, :id => @competition.id} %>
        </div>
      </div>
    </div>
  </div>

  <%= render :partial => 'competitions_menu' %>

  <div id="page_panel">
    <div class="courses">
      <div class="main_column">
        <h2 class="first_card_section">Courses</h2>
      
      ...test...

      </div>
      <div class="side_column">
        <div class="section">
          <h3>Search Statistics</h3>
          <div class="bar_chart simple">
            <h2 class="header">
              <span class="title"><%= @courses_count -%></span>
              <span class="title_label">Total Courses</span>
              <span class="sub_title"/>
              <div style="clear: left; height: 0px;"/>
            </h2>
          </div>
        </div>
      </div>
    </div>
  </div>
