<fb:tabs>
	<fb:tab-item href="<%= url_for :controller => :profile, :action => :index %>" title="Profile" selected='<%= @action == :profile %>'/>
        <fb:if-is-user uid="<%=@user.facebook_uid%>">
            <fb:tab-item href="<%= url_for :controller => :profile, :action => :location %>" title="Location" selected='<%= @action == :location %>' />
	</fb:if-is-user>
        <fb:tab-item href="<%= user_view_rounds_url :id => @user.id, :only_path => true %>" title="Rounds" selected='<%= @action == :rounds %>' />
        <%if @user.admin%>
            <fb:tab-item href="<%= url_for :action => :users, :id => @user.id, :only_path => true %>" title="Users" selected='<%= @action == :users %>' />
        <%end%>
  </fb:tabs>