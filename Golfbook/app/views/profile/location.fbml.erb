<div class="module clearfix page_panel_header no_border">
	<div class="header_page_summary clearfix">
		<div class="thumb">
			<fb:profile-pic uid="<%= fbsession.session_user_id %>" size="square" linked="true" />
		</div>
		<div class="info">
			<h2><fb:name uid="<%= fbsession.session_user_id %>"shownetwork="true" useyou="false" /></h2>
		</div>
	</div>
</div>
<fb:tabs>
	<fb:tab-item href="<%= url_for :controller => :profile, :action => :index %>" title="Profile" />
	<fb:tab-item href="<%= url_for :controller => :profile, :action => :location %>" title="My Location" selected="true" />
</fb:tabs>

<!-- Type ahead script -->
<script src="http://993440e2.fb.joyent.us:8889/javascripts/typeahead.js"></script>
<script>
<!--
	options = {
		preMsgTxt: "type for suggestions",	// text to display when nothing has been typed
