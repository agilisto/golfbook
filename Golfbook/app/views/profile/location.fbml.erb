</script>

<div class="editor_panel">
	<div class="editor clearfix">
			<div class="city_input">
				<% form_for :user, :url => {:action => :process_location} do |f| %>
				<span class="label">City Name</span>

				<div id="lookup">
				<%= f.text_field :address, {:size => 100, :id => 'i_like_text', :class => 'inputtext'} %>
				<script>
				<!--
					var suggestr = new ajaxSuggestFbml(document.getElementById('i_like_text'), options);
				-->
				</script>
				</div>
				

				<div class="buttons">
					<%= submit_tag %>
				</div>
				<% end%>
			</div>
		</div>
	</div>
</div>
